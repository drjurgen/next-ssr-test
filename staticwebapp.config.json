{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{png,jpg,gif,svg}", "*.css"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".html": "text/html",
    ".js": "text/javascript"
  },
  "routes": [
    {
      "route": "/api/*",
      "headers": {
        "Access-Control-Allow-Headers": "X-Authorization"
      }
    }
  ]
}
